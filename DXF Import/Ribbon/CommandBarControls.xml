                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="urn:abb-robotics-robotstudio-commandbarcontrols file:///C:\Program%20Files%20(x86)\ABB\RobotStudio%202021\Schemas\CommandBarControls.xsd">

	<!-- Controls defined by this add-in -->

	<!-- The 'Start' button has defaultEnabled set to true which means it will be enabled even before the add-in is loaded. -->
