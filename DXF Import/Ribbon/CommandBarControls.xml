	<CommandBarComboBox id="DXF_Import.ActiveIOs" caption="Plasma IO"/>
	<CommandBarComboBox id="DXF_Import.PeirceTime" caption="Peirce Time"/>
	<CommandBarComboBox id="DXF_Import.CutSpeed" caption="Cut Speed"/>
